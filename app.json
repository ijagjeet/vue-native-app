{
  "expo": {
    "sdkVersion": "35.0.0",
    "icon": "./assets/nblogo.png",
    "slug": "vue-native-app",
    "loading": {
      "hideExponentText": true,
      "icon": "./assets/nblogo.png",
      "backgroundImage": "./assets/splashscreen.png"
    },
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "packagerOpts": {
      "sourceExts": [
        "vue",
        "js",
        "json",
        "ts",
        "tsx"
      ],
      "assetExts": [
        "ttf"
      ]
    },
    "androidStatusBarColor": "#334393",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#334393"
    },
    "name": "VUE Native APP",
    "version": "0.1.0",
    "platforms": [
      "android",
      "ios"
    ],
    "description": "",
    "githubUrl": "https://github.com/ijagjeet/vue-native-app",

    "ios": {
      "bundleIdentifier": "com.ijagjeet.vue-native-app"
    },
    "android": {
      "package": "com.ijagjeet.vue-native-app"
    }
  }
